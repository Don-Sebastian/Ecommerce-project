<!-- Content wrapper -->
          <div class="content-wrapper">
            <!-- Content -->

            <div class="container-xxl flex-grow-1 container-p-y">
              <h4 class="fw-bold py-3 mb-4"><span class="text-muted fw-light">Categories/</span> Edit Sub Category</h4>

              <!-- Basic Layout -->
              <div class="row">
                <div class="col-xl">
                  <div class="card mb-4">
                    <div class="card-header d-flex justify-content-between align-items-center">
                      <h5 class="mb-0">Edit Sub Category</h5>
                      <small class="text-muted float-end"></small>
                    </div>
                    <div class="card-body">
                      
                      <form action="/admin/edit-sub-category/<%= subcategory[0].subCategory._id %>" method="POST" role="form" >
                        <div class="mb-3">
                          <label class="form-label" for="basic-default-fullname">Name</label>
                          <input type="text" name="CategoryName" class="form-control" id="basic-default-fullname" value="<%= subcategory[0].subCategory.CategoryName %>" required />
                        </div>

                        <div class="mb-3">
                          <label class="form-label" for="basic-default-message">Sub Category Name</label>
                          <input
                            id="basic-default-message"  name="subCategoryName"
                            class="form-control"
                            value="<%= subcategory[0].subCategory.subCategoryName %>" required
                          ></input>
                        </div>
                                                
                        <div class="mb-3">
                          <label class="form-label" for="basic-default-message">Sub Category Description</label>
                          <input
                            id="basic-default-message"  name="subCategoryDescription"
                            class="form-control"
                            value="<%= subcategory[0].subCategory.subCategoryDescription %>" required
                          ></input>
                        </div>
                        
                        <button type="submit" class="btn btn-primary ">Update Sub Category</button>
                      </form>
                    </div>
                  </div>
                </div>
                  
                  
                </div>
              </div>
            </div>
            <!-- / Content -->

          